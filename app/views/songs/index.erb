<h1>All The Songs</h1>


<a href="/">Home Page</a>
<a href="/songs/new">Post a new song!</a>

<% @songs.each do |song| %>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.title %></a> <small>(by: <%= song.author %>)</small></h3>
    <p>
      <h5><small>URL: <a href=<%= song.url %>>view their site</a></small></h5>
    </p>
  </div>
<% end %>

<hr/>
<a href="/songs/new">Post a song!</a>